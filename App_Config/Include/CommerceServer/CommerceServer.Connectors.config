<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>

    <commerce.Entities>
      <Party>
        <patch:attribute name="type">Sitecore.Commerce.Connect.CommerceServer.Orders.Models.CommerceParty, Sitecore.Commerce.Connect.CommerceServer</patch:attribute>
      </Party>
    </commerce.Entities>

    <itemClassificationService type="Sitecore.Commerce.Products.ItemClassificationService, Sitecore.Commerce">
      <patch:attribute name="type">
        Sitecore.Commerce.Connect.CommerceServer.Catalog.ItemClassificationService, Sitecore.Commerce.Connect.CommerceServer
      </patch:attribute>
    </itemClassificationService>

    <commerceContext type="Sitecore.Commerce.Multishop.CommerceContextBase, Sitecore.Commerce">
      <patch:attribute name="type">
        Sitecore.Commerce.Connect.CommerceServer.CommerceConnectContext, Sitecore.Commerce.Connect.CommerceServer
      </patch:attribute>
    </commerceContext>
  </sitecore>
</configuration>