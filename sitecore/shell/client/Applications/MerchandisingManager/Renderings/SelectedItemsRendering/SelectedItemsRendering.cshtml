@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@using Sitecore.Web.UI.Controls.Common.UserControls
@model RenderingModel
@{
  var rendering = Html.Sitecore().Controls().GetUserControl(Model.Rendering);
  rendering.Class = "sc-SelectedItemsRendering";
  rendering.Requires.Script("client", "SelectedItemsRendering.js");

  rendering.GetString("SelectedItemString", "SelectedItemString");
  rendering.GetString("SelectedItemObject", "SelectedItemObject");
  
  var htmlAttributes = rendering.HtmlAttributes;
}      
<div @htmlAttributes>
</div>
